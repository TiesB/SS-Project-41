  <path id="javac.classpath">
    <pathelement location="lib/" />
  </path>

  <javac srcdir="src/" classpathref="javac.classpath">

  </javac>
  
  <!-- Modules -->
  
  <import file="${basedir}/module_ss-project-41.xml"/>
  
  <target name="init" description="Build initialization">
    <!-- Perform any build initialization in this target -->
  </target>
  
  <target name="clean" depends="clean.module.ss-project-41" description="cleanup all"/>
  
  <target name="build.modules" depends="init, clean, compile.module.ss-project-41" description="build all modules"/>
  
  <target name="all" depends="build.modules" description="build all"/>
</project>